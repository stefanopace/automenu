{
    "type": "list",
    "returns": "action",
    "options": ["Hello", "Exit"],
    "resolvers": [
        {
            "type": "list",
            "returns": "name",
            "match": ["Hello"],
            "options": ["World", "Man", "Pippo", "Johnny"],
            "resolvers": [ {
                    "type": "leaf",
                    "match" : ["World"],
                    "execute": {
                        "lang": "sh",
                        "commands": ["echo Hello World!"]
                    }
                },{
                    "type": "leaf",
                    "match" : ["Man"],
                    "execute": {
                        "lang": "sh",
                        "commands": ["echo Yo man!"]
                    }
                },{
                    "type": "leaf",
                    "execute": {
                        "lang": "sh",
                        "commands": ["echo \"Who are you {name}?\""]
                    }
            } ]
        },
        {
            "type": "leaf",
            "match": "Exit",
            "execute": {
                "lang": "sh",
                "commands": ["echo \"Bye!\""]
            }
        }
    ]
}