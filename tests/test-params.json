{
    "type": "list",
    "returns": "language",
    "options": ["JavaScript", "Python", "Elixir"],
    "resolvers": [ {
        "type": "list",
        "returns": "opinion",
        "options": ["sucks", "is awesome"],
        "resolvers": [
            {
                "type": "leaf",
                "execute": ["echo {language} {opinion}!"]
            }
        ]
    } ]
}